unroll coordinates

Global variables
totalPoints = 361
margintop  = 25
marginleft = radius+25
radius      = 50
cx          = radius/2 + marginleft
cy          = radius/2 + margintop
duration    = 1000      //in ms

circle
totalPoints = 361  //should have one more to connect the circle, NOT using z to close it
circumference = 2 * radius * Math.PI;  //also lineLength
coordinate x of n =
 marginleft + radius + radius * Math.sin(2 * n * Math.PI / (numberOfPoints - 1))
coordinate y of n =
 margintop + radius - radius * Math.cos(2 * n * Math.PI / (numberOfPoints - 1))

line
var lineLength = 2 * radius * Math.PI;
lineDivision = lineLength/totalPoints
line cooridinate (x) of point n = marginleft + lineLength/totalPoints * n
line coordinate (y) of point n = margintop

stateOfUnroll
  var circleStates = [];
  for (i=0; i<totalPoints; i++){
    var individualState = $.map(Array(totalPoints-i), function (d, j) {
      x = marginleft + radius + lineDivision*j + radius * Math.sin(2 * j * Math.PI / (numberOfPoints - 1));
      y =  margintop + radius - radius * Math.cos(2 * j * Math.PI / (numberOfPoints - 1));
      return { x: x, y: y};
    })
    circleStates.push(individualState);
  }
  0 
    Full circle
    shift = 0
    x = marginleft + radius + lineDivision*0 + radius * Math.sin(2 * n * Math.PI / (numberOfPoints - 1))
    y =  margintop + radius - radius * Math.cos(2 * n * Math.PI / (numberOfPoints - 1))
  1 
    origin remains same
    shift = 1
    x = marginleft + radius + lineDivision*1 + radius * Math.sin(2 * n * Math.PI / (numberOfPoints - 1 -1))
    y =  margintop + radius - radius * Math.cos(2 * n * Math.PI / (numberOfPoints - 1 -1))
  2 
    origin remains same
    1 remains same
    shift = 2
    x = marginleft + radius + lineDivision*2 + radius * Math.sin(2 * n * Math.PI / (numberOfPoints - 1 -2))
    y =  margintop + radius - radius * Math.cos(2 * n * Math.PI / (numberOfPoints - 1 -2))
  n 
    origin remains same
    n-1 remains same
    shift = n
    x = marginleft + radius + lineDivision*n + radius * Math.sin(2 * n * Math.PI / (numberOfPoints - 1 -n))
    y =  margintop + radius - radius * Math.cos(2 * n * Math.PI / (numberOfPoints - 1 -n))
  length Full Line
    origin remains same
    totalPoints-1 remains same
    shift = 04
    x = linelength
    y = margin top

