funtion

function unrollAll() {
  for ()
    circle.data([circleData2])
        .transition()
        .duration(dur)
        .ease("linear")
        .attr('d', pathFunction)
    circle.data([circleData3])
        .transition()
        .delay(dur*1)
        .duration(dur)
        .ease("linear")
        .attr('d', pathFunction)
    circle.data([circleData4])
        .transition()
        .delay(dur*2)
        .duration(dur)
        .ease("linear")
        .attr('d', pathFunction)
    circle.data([circleData5])
        .transition()
        .delay(dur*3)
        .duration(dur)
        .ease("linear")
        .attr('d', pathFunction)
}
